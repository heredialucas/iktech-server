{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-useless-escape": "off",
    "key-spacing": "off",
    "no-console": "error"
  }
}
